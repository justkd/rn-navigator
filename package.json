{
  "name": "template-expo-ts-npm-publish",
  "version": "1.0.0",
  "main": "./node_modules/expo/AppEntry",
  "keywords": [
    "rn",
    "react native",
    "react-native",
    "responsive",
    "responsiveness"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/justkd/template-expo-ts-npm-publish"
  },
  "license": "MIT",
  "scripts": {
    "start": "export EXPO_PACKAGER_PROXY_URL=https://${CSB_SANDBOX_ID}-8080.csb.app && expo start --port 8080",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "npm run build:lib && npm run build:copy && npm run build:minify",
    "build:lib": "rm ./publish/lib -d -r -f && npx tsc",
    "build:copy": "cp README.md publish && cp LICENSE publish",
    "build:minify": "npx minify-all-js ./publish",
    "publish:private": "cd publish && npm run publish:private",
    "publish:public": "cd publish && npm run publish:public"
  },
  "dependencies": {
    "@expo/cli": "^0.6.2",
    "expo": "~49.0.16",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.6",
    "react-native-web": "~0.19.6",
    "typescript": "^5.1.3",
    "@types/react": "~18.2.14"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "body-parser": "^1.18.3",
    "eslint": "^8.52.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "express": "^4.16.3",
    "http-proxy-middleware": "^0.19.0",
    "minify-all-js": "^0.1.9",
    "qs": "^6.5.2",
    "typescript": "^5.1.3",
    "xdl": "^60.0.12"
  },
  "private": true
}
